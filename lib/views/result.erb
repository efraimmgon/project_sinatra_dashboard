<h1>Result</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Index</th>
      <th>Position</th>
      <th>Company Name</th>
      <th>Job Link</th>
      <th>Description</th>
      <th>Location</th>
      <th>Posted at</th>
      <th>Company Link</th>
    </tr>
  </thead>
  <tbody>
  <% result.each_with_index do |job, i| %>
    <tr>
      <td><%= i + 1 %></td>
      <% job.each do |item| %>
      <td><%= item %></td>
      <% end %>
    </tr>
  <% end %>
  </tbody>
</table>
